{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "bin/**/*",
            "dist/**/*",
            "node_modules/**/*",
            "jest.config.js",
            "stryker.conf.js"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-console": false,
        "max-line-length": [true, 160],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
